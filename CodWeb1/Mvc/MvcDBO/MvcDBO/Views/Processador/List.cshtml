@model IEnumerable<MvcDBO.Models.Processador>

@{
    ViewBag.Title = "List";
}

<h2>Lista</h2>
<h4>Processador</h4>
<p>
    @Html.ActionLink("Novo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Marca)
