@model IEnumerable<MvcDBO.Models.PlacaMae>

@{
    ViewBag.Title = "List";
}

<h2>Lista</h2>
<h4>Placa Mães</h4>
<p>
    @Html.ActionLink("Novo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Marca)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Modelo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TipoBarramentoMemoriaRam)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.QtBarramentoMemoriaRam)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TipoBarramentoArmazenamento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.QtBarramentoArmazenamento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Soquete)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Chipset)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Marca)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Modelo)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TipoBarramentoMemoriaRam)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.QtBarramentoMemoriaRam)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TipoBarramentoArmazenamento)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.QtBarramentoArmazenamento)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Soquete)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Chipset)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
    </td>
</tr>
}

</table>
