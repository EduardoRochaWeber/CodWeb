@model IEnumerable<MvcDBO.Models.Armazenamento>

@{
    ViewBag.Title = "List";
}

<h2>Lista</h2>
<h4>Armazenamento</h4>
<p>
    @Html.ActionLink("Novo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Marca)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Modelo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TipoArmazenamento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Medidas)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TipoBarramento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TamanhoArmazenamento)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Marca)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Modelo)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TipoArmazenamento)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Medidas)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TipoBarramento)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TamanhoArmazenamento)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
    </td>
</tr>
}

</table>
