@model List<WebMvc.Models.Carros>
@{
    ViewBag.Title = "Listar";
}

<div id="consulta">
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Marca</th>
                <th>Modelo</th>
            </tr>

        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Marca</td>
                    <td>@item.Modelo</td>
                    <td><a href="/Home/Deletar/@item.Id">Deletar</a></td>
                </tr>
            }
        </tbody>
    </table>
</div>